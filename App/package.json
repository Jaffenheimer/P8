{
  "name": "App",
  "version": "1.0.0",
  "scripts": {
    "android": "expo start --android",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "ios": "expo start --ios",
    "start": "expo start",
    "startt": "expo start --tunnel",
    "welb": "expo start --web",
    "test": "jest" 
  },
  "jest": {
    "preset": "jest-expo",
