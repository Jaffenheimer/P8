<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    <vType id="minibus" length="7.00" maxSpeed="13.90" desiredMaxSpeed="13.90" vClass="bus" personCapacity="24" />
    <route id="r_0" edges="-overlap -R2 -R1 -R0" color="yellow" cycleTime="300.00" repeat="100">
        <stop busStop="bs_road_-overlap" duration="5.00" />
        <stop busStop="bs_road_-R2" duration="5.00" />
        <stop busStop="bs_road_-R1" duration="5.00" />
        <stop busStop="bs_road_-R0" duration="5.00" />
    </route>
    <route id="r_1" edges="-overlap -L3 -L2 -L1 -L0" color="blue" cycleTime="300.00" repeat="100">
        <stop busStop="bs_road_-overlap" duration="5.00" />
        <stop busStop="bs_road_-L3" duration="5.00" />
        <stop busStop="bs_road_-L2" duration="5.00" />
        <stop busStop="bs_road_-L1" duration="5.00" />
        <stop busStop="bs_road_-L0" duration="5.00" />
    </route>
    <!-- now the busses all spawn at the same time, but 10 meters apart -->
    <vehicle id="bus_r_0_0" type="minibus" departPos="10" depart="0" color="yellow" route="r_0" />
    <vehicle id="bus_r_1_0" type="minibus" departPos="20" depart="0" color="green" route="r_1" />
    <vehicle id="bus_r_0_1" type="minibus" departPos="30" depart="0" color="yellow" route="r_0" />
    <vehicle id="bus_r_1_1" type="minibus" departPos="40" depart="0" color="green" route="r_1" />
    <vehicle id="bus_r_0_2" type="minibus" departPos="50" depart="0" color="yellow" route="r_0" />
    <vehicle id="bus_r_1_2" type="minibus" departPos="60" depart="0" color="green" route="r_1" />
    <vehicle id="bus_r_0_3" type="minibus" departPos="70" depart="0" color="yellow" route="r_0" />
    <vehicle id="bus_r_1_3" type="minibus" departPos="80" depart="0" color="green" route="r_1" />
    <vehicle id="bus_r_0_4" type="minibus" departPos="90" depart="0" color="yellow" route="r_0" />
    <vehicle id="bus_r_1_4" type="minibus" departPos="100" depart="0" color="green" route="r_1" />
</routes>