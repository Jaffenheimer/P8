<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">

    <vType id="car" desiredMaxSpeed="13.90" vClass="passenger" />

    <!-- Cars in the right lane -->
    <flow id="carROverlap" type="car" begin="50" color="red" vehsPerHour="100" from="-overlap" to="-R0">
    </flow>

    <flow id="carR2" type="car" begin="50" color="red" vehsPerHour="100" from="-R2" to="-overlap">
    </flow>

    <flow id="carR1" type="car" begin="50" color="red" vehsPerHour="100" from="-R1" to="-R2">
    </flow>

    <flow id="carR0" type="car" begin="50" color="red" vehsPerHour="100" from="-R0" to="-R1">
    </flow>

    <!-- Cars in the left lane -->
    <flow id="carLOverlap" type="car" begin="50" color="blue" vehsPerHour="100" from="-overlap" to="-L0">
    </flow>

    <flow id="carL3" type="car" begin="50" color="blue" vehsPerHour="100" from="-L3" to="-overlap">
    </flow>

    <flow id="carL2" type="car" begin="50" color="blue" vehsPerHour="100" from="-L2" to="-L3">
    </flow>

    <flow id="carL1" type="car" begin="50" color="blue" vehsPerHour="100" from="-L1" to="-L2">
    </flow>

    <flow id="carL0" type="car" begin="50" color="blue" vehsPerHour="100" from="-L0" to="-L1">
    </flow>

</routes>